{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "additionalProperties": false,
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "company"
            ]
        },
        "minEmployees": {
            "type": "string",
            "default": "",
            "title": "The minEmployees Schema",
            "examples": [
                "1"
            ],
            "pattern": "^\\d+$"
        },
        "maxEmployees": {
            "type": "string",
            "default": "",
            "title": "The maxEmployees Schema",
            "examples": [
                "2"
            ],
            "pattern": "^\\d+$"
        }
    },
    "examples": [{
        "name": "company",
        "minEmployees": "1",
        "maxEmployees": "2"
    }]
}